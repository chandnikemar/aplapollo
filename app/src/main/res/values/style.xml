<?xml version="1.0" encoding="utf-8"?>
<resources><style name="TableCellStyle">
    <item name="android:layout_width">0dp</item>
    <item name="android:layout_weight">1</item>
    <item name="android:layout_height">40dp</item>
    <item name="android:gravity">center</item>
    <item name="android:background">@drawable/rectangle_background</item>
    <item name="android:textColor">@color/grey</item>
    <item name="android:padding">8dp</item>
</style>
    <style name="AppTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar" />
    <style name="QcLabel">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">14sp</item>
        <item name="android:padding">8dp</item>
        <item name="android:textColor">#000</item>
    </style>

    <style name="QcValue">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">14sp</item>
        <item name="android:padding">8dp</item>
        <item name="android:background">@drawable/table_value_bg</item>
        <item name="android:textColor">#000</item>
    </style>
        <style name="ShapeAppearanceOverlay.MyTextInput" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">12dp</item>
    </style>
    <style name="GridItem">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_columnWeight">1</item>
        <item name="android:padding">10dp</item>
        <item name="android:background">@drawable/rectangle_background</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/black</item>

    </style>
    <style name="spcLabel">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">14sp</item>

        <item name="android:textColor">#444444</item>
    </style>

    <style name="spcValue">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">14sp</item>

        <item name="android:textColor">#000000</item>
    </style>

    <style name="spcValueHighlight">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">14sp</item>

        <item name="android:textColor">#FFFFFF</item>
        <item name="android:background">#1565C0</item>
    </style>

    <style name="outputSquare">
        <item name="android:layout_width">44dp</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_margin">6dp</item>
        <item name="android:background">@drawable/card_green_gradient</item>
    </style>



</resources>